<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LogisticForStudent.Views.Listadetalles">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center">
            <Image x:Name="MyImagencall"
                   HeightRequest="100"
                   WidthRequest="100" />
            <Label x:Name="Detalles"
                   Margin="20,20"
                   FontSize="Small" />
            <Label x:Name="HorariosShow"
                   Margin="20,20"
                   FontSize="Small" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>